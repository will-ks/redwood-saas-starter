{
  "name": "{{project_name}}",
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "scripts": {
    "dev": "yarn check-env && yarn rw dev",
    "check-env": "yarn rw exec check-env",
    "pre-deploy": "yarn check-env"
  },
  "dependencies": {
    "@arrows/composition": "1.2.2",
    "@will-ks/helpers": "1.0.1",
    "axios": "1.2.1"
  },
  "devDependencies": {
    "@redwoodjs/core": "3.5.0",
    "execa": "5.1.1",
    "prettier-plugin-prisma": "4.2.0",
    "type-flag": "3.0.0"
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@3.2.1"
}
