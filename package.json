{
  "name": "redwood-saas-starter",
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "scripts": {
    "dev": "yarn check-env && yarn rw dev",
    "build": "yarn check-env && rw build --verbose",
    "serve": "yarn build && echo \"Build successful\" && yarn rw serve",
    "check-env": "yarn rw exec check-env",
    "pre-deploy": "yarn check-env",
    "start:cluster-ephemeral": "nomad job run redwood-saas-starter-ephemeral.nomad",
    "stop:cluster-ephemeral": "nomad job stop redwood-saas-starter-ephemeral",
    "nomad-ui": "open http://localhost:4646",
    "temporal-ui": "open http://localhost:8233",
    "cluster-ui": "yarn nomad-ui && yarn temporal-ui"
  },
  "devDependencies": {
    "@redwoodjs/auth-supertokens-setup": "4.1.0-rc.263",
    "@redwoodjs/core": "^4.1.0-rc.263",
    "esbuild": "0.17.8",
    "eslint-plugin-compat": "4.0.1",
    "eslint-plugin-no-unsanitized": "4.0.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-unicorn": "40.1.0",
    "prettier-plugin-prisma": "4.2.0",
    "type-flag": "3.0.0"
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "packageManager": "yarn@3.2.1",
  "browserslist": [
    "last 2 Chrome major versions",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions"
  ]
}
